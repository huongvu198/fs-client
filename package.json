    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "sass": "^1.86.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  },
  "lint-staged": {
    "**/*.{ts,tsx,scss,json}": [
      "npm run prettier"
    ],
    "**/*.{ts,tsx}": [
      "npm run lint"
