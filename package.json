    "classnames": "^2.5.1",
    "i18next": "^23.11.2",
    "js-cookie": "^3.0.5",
    "motion": "^12.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.1",
    "react-query": "^3.39.3",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.22.3",
    "slick-carousel": "^1.8.1",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.17.16",
